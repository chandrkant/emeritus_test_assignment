<table class="table table-striped table-bordered table-hover" id="list-data">
  <thead>
    <tr>
        <th>Email</th>
        <th>Name</th>
        <th>Gender</th>
        <th>Created At</th>
        <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @school_admins.each do |school_admin| %>
      <tr>
        <td><%= school_admin.email %></td>
        <td><%= school_admin.name %></td>
        <td><%= school_admin.gender %></td>
        <td><%= school_admin.created_at %></td>       
        <td>
          <%= link_to ('<i class="fa fa-edit"></i>').html_safe, edit_school_admin_path(school_admin.id), title: "Edit" %>  
          <%= link_to ('<i class="fa fa-eye"></i>').html_safe, school_admin_path(school_admin.id), title: "Show" %> 
         </td>
      </tr>
    <% end %>
  </tbody>
</table>
